{
  "name": "fiddly",
  "description": "Create beautiful and simple HTML pages from your Readme.md files",
  "version": "0.8.0",
  "bin": {
    "fiddly": "bin/fiddly.js"
  },
  "scripts": {
    "format": "prettier --write \"src/**/*.js\"",
    "test": "jest",
    "posttest": "npm run format",
    "watch": "jest --watch",
    "coverage": "jest --coverage",
    "build": "./bin/fiddly.js",
    "prepublish": "npm run test",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "now-build": "npm run build"
  },
  "files": [
    "LICENSE",
    "Readme.md",
    "bin",
    "src"
  ],
  "keywords": [
    "generation",
    "markdown",
    "static",
    "sites",
    "fiddly"
  ],
  "author": {
    "name": "Sara Vieira",
    "email": "hey@iamsaravieira.com",
    "url": "https://iamsaravieira.com/"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.2",
    "clean-css": "^4.2.3",
    "create-html": "^4.1.0",
    "gh-pages": "^3.0.0",
    "gluegun": "4.3.1",
    "imagemin": "^7.0.1",
    "imagemin-jpegtran": "^7.0.0",
    "imagemin-pngquant": "^9.0.0",
    "markdown-it": "^11.0.0",
    "markdown-it-anchor": "^5.3.0",
    "markdown-it-checkbox": "^1.1.0",
    "markdown-it-emoji": "^1.4.0",
    "markdown-it-for-inline": "^0.1.1",
    "markdown-it-github-headings": "^2.0.0",
    "markdown-it-inline-comments": "^1.0.1",
    "markdown-it-prism": "^2.0.5",
    "markdownlint": "^0.20.3",
    "node-sass": "^4.14.1",
    "normalize.css": "^8.0.1",
    "snarkdown": "^1.2.2",
    "to-css": "^1.2.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^6.15.0",
    "eslint": "^7.1.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.0.1",
    "prettier": "^2.0.5"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "homepage": "https://github.com/SaraVieira/fiddly",
  "bugs": {
    "url": "https://github.com/SaraVieira/fiddly/issues",
    "email": "hey@iamsaravieira.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SaraVieira/fiddly"
  }
}
